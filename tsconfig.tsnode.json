{
    "compilerOptions": {
      "target": "ES2017",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "strict": true,
      "paths": {
        "@/*": ["./src/*"]
      }
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules"]
  }
  